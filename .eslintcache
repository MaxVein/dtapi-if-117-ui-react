en: true,\n        message: \"Групу вибрано\",\n      });\n      setSubmitted(true);\n      setSelectedGroupId(id);\n    } else {\n      setSelectedGroupId(null);\n      setSubmitted(false);\n      setSnackBar({\n        open: true,\n        message: \"Сталася помилка при отриманні даних! Спробуйте знову\",\n      });\n    }\n  };\n\n  return (\n    <div className={classes.Container}>\n      <div className={classes.Faculties}>\n        <div className={classes.Selector}>\n          <FormControl fullWidth={true} className={classes.FormControl}>\n            <InputLabel\n              classes={{ shrink: \"right: unset;\" }}\n              className={classes.Label}\n            >\n              <SchoolIcon className={classes.SelectorIcon} />\n              Виберіть Факультет/Інститут\n            </InputLabel>\n            <Select\n              placeholder=\"Факультет/Інститут\"\n              required\n              value={faculty}\n              className={classes.Select}\n              onChange={async (event) => {\n                setFaculty(event.target.value);\n                setSnackBar({ open: true, message: \"Факультет вибрано\" });\n                await getGroupsList(+event.target.value);\n              }}\n              disabled={!faculties.length}\n            >\n              {faculties.map(({ faculty_id, faculty_name }) => (\n                <MenuItem key={faculty_id} value={+faculty_id}>\n                  {faculty_name}\n                </MenuItem>\n              ))}\n            </Select>\n          </FormControl>\n        </div>\n      </div>\n      <div className={classes.Groups}>\n        <div className={classes.Selector}>\n          <FormControl fullWidth={true} className={classes.FormControl}>\n            <InputLabel className={classes.Label}>\n              <GroupIcon className={classes.SelectorIcon} />\n              Виберіть Групу\n            </InputLabel>\n            <Select\n              placeholder=\"Група\"\n              required\n              value={group}\n              className={classes.Select}\n              onChange={(event) => {\n                setGroup(event.target.value);\n                getGroup(+event.target.value);\n              }}\n              disabled={(!faculties.length && !groups.length) || faculty === \"\"}\n            >\n              {groups.map(({ group_id, group_name }) => (\n                <MenuItem key={group_id} value={+group_id}>\n                  {group_name}\n                </MenuItem>\n              ))}\n            </Select>\n          </FormControl>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default TransferSelects;\n\nTransferSelects.propTypes = {\n  setSubmitted: PropTypes.func,\n  setSelectedGroupId: PropTypes.func,\n  errorHandler: PropTypes.func,\n  setSnackBar: PropTypes.func,\n};\n",{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":55,"column":6,"nodeType":"137","endLine":55,"endColumn":57,"suggestions":"138"},{"ruleId":"135","severity":1,"message":"136","line":118,"column":6,"nodeType":"137","endLine":128,"endColumn":4,"suggestions":"139"},{"ruleId":"135","severity":1,"message":"140","line":63,"column":6,"nodeType":"137","endLine":63,"endColumn":26,"suggestions":"141"},{"ruleId":"135","severity":1,"message":"136","line":52,"column":6,"nodeType":"137","endLine":52,"endColumn":15,"suggestions":"142"},{"ruleId":"135","severity":1,"message":"143","line":32,"column":6,"nodeType":"137","endLine":32,"endColumn":8,"suggestions":"144"},"no-native-reassign",["145"],"no-negated-in-lhs",["146"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'errorHandler'. Either include it or remove the dependency array.","ArrayExpression",["147"],["148"],"React Hook useEffect has missing dependencies: 'setError', 'setOpen', and 'setSnackBar'. Either include them or remove the dependency array. If 'setSnackBar' changes too often, find the parent componen